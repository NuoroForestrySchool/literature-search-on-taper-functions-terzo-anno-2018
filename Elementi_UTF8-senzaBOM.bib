@article{machado_comparacao_2005,
	title = {Compara{\c c}{\~a}o de m{\'e}todos de estimativa de volume para {Pinus} oocarpa em diferentes idades e diferente iregimes de desbastes},
	volume = {2005},
	url = {https://pfb.cnpf.embrapa.br/pfb/index.php/pfb/article/view/242/193},
	abstract = {Esta pesquisa visa comparar os volumes dos fustes de  P. oocarpa  obtidos pelos m{\'e}todos de Smalian, fator de forma, equa{\c c}{\~a}o de volume e fun{\c c}{\~a}o de afilamento. Foram usadas 1100 {\'a}rvores distribu{\'i}das em 11 regimes~ e manejo pertencentes a Duraflora S. A. de Agudos {\textendash} SP. As {\'a}rvores foram cubadas e os seus volumes com e sem casca foram estimados pelos~ m{\'e}todos citados. Para verificar a homogeneidade de vari{\^a}ncias, aplicou-se o Teste de Bartlett. A an{\'a}lise de vari{\^a}ncia mostrou pela compara{\c c}{\~a}o do valor de F e~ n{\'i}vel de signific{\^a}ncia, a n{\~a}o exist{\^e}ncia de diferen{\c c}as significativas entre os m{\'e}todos de estimativa de~~ volume para os 11 regimes de manejo com e sem casca, ao n{\'i}vel de 95\% de probabilidade,~ confirmando a hip{\'o}tese de que os volumes m{\'e}dios estimados atrav{\'e}s dos m{\'e}todos estudados s{\~a}o~ estatisticamente iguais entre si.},
	number = {50 ( jan./jun.)},
	journal = {Pesquisa Florestal Brasileira},
	author = {Machado, Sebasti{\~a}o do Amaral and Urbano, Edilson and Concei{\c c}{\~a}o, Marcio Barbosa da},
	year = {2005},
	keywords = {equa{\c c}{\~a}o de afilamento, equa{\c c}{\~a}o de volume, fator de forma, Volume por Smalian}
}

