@article{machado_comparacao_2005,
  title = {Comparação de Métodos de Estimativa de Volume Para {{Pinus}} Oocarpa Em Diferentes Idades e Diferente Iregimes de Desbastes},
  volume = {2005},
  url = {https://pfb.cnpf.embrapa.br/pfb/index.php/pfb/article/view/242/193},
  abstract = {Esta pesquisa visa comparar os volumes dos fustes de  P. oocarpa  obtidos pelos métodos de Smalian, fator de forma, equação de volume e função de afilamento. Foram usadas 1100 árvores distribuídas em 11 regimes~ e manejo pertencentes a Duraflora S. A. de Agudos – SP. As árvores foram cubadas e os seus volumes com e sem casca foram estimados pelos~ métodos citados. Para verificar a homogeneidade de variâncias, aplicou-se o Teste de Bartlett. A análise de variância mostrou pela comparação do valor de F e~ nível de significância, a não existência de diferenças significativas entre os métodos de estimativa de~~ volume para os 11 regimes de manejo com e sem casca, ao nível de 95\% de probabilidade,~ confirmando a hipótese de que os volumes médios estimados através dos métodos estudados são~ estatisticamente iguais entre si.},
  issue = {50 ( jan./jun.)},
  journaltitle = {Pesquisa Florestal Brasileira},
  date = {2005},
  keywords = {equação de afilamento,equação de volume,fator de forma,Volume por Smalian},
  author = {Machado, Sebastião do Amaral and Urbano, Edilson and da Conceição, Marcio Barbosa}
}

